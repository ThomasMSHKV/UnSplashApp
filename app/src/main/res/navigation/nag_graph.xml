<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nag_graph"
    app:startDestination="@id/openSearchFragment">


    <fragment
        android:id="@+id/openSearchFragment"
        android:name="com.example.unsplashapp.OpenSearchFragment"
        android:label="fragment_open_search"
        tools:layout="@layout/fragment_open_search" >
        <action
            android:id="@+id/action_openSearchFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.unsplashapp.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_openSearchFragment"
            app:destination="@id/openSearchFragment" />
    </fragment>

</navigation>